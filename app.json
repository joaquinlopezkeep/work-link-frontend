{
	"expo": {
		"name": "Work Link",
		"slug": "work-link",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/ic_launcher.png",
		"splash": {
			"image": "./assets/playstore.png",
			"resizeMode": "contain",
			"backgroundColor": "#000000"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"package": "birkbeck.worklink",
			"adaptiveIcon": {
				"foregroundImage": "./assets/ic_launcher.png",
				"backgroundColor": "#000000"
			}
		},
		"web": {
			"favicon": "./assets/favicon.png"
		}
	}
}
